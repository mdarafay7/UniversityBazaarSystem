        <xs:attribute ref="and:layout_marginBottom" xmlns:and="http://schemas.android.com/apk/res/android"/>
        <xs:attribute ref="and:ems" xmlns:and="http://schemas.android.com/apk/res/android"/>
        <xs:attribute ref="and:hint" xmlns:and="http://schemas.android.com/apk/res/android"/>
        <xs:attribute ref="and:inputType" xmlns:and="http://schemas.android.com/apk/res/android"/>
        <xs:attribute ref="res:layout_constraintBottom_toBottomOf" xmlns:res="http://schemas.android.com/apk/res-auto"/>
