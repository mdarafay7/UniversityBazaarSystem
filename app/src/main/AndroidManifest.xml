<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.ubs">

    <uses-permission android:name="android.permission.INTERNET" />


    <application
        android:name=".FireApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".ImagesActivity"></activity>
        <activity android:name=".Login">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".CreateAccount" />
        <activity android:name=".Clubs" />
        <activity android:name=".Forum" />
        <activity android:name=".CreateClub" />
        <activity android:name=".Messaging" />
        <activity android:name=".UsrMsg" />
        <activity android:name=".UsrSendMsg" />
        <activity android:name=".ClubMsg" />
        <activity android:name=".Inbox" />
        <activity android:name=".ExchangePost" />
        <activity android:name=".post" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>